{
  "name": "Home Assistant Garage Door",
  "domain": "homebridge-homeassistant-garagedoor",
  "exclusive": true,
  "type": "object",
  "properties": {
    "name": {
      "title": "Name",
      "required": true,
      "type": "string",
      "default": "Garage Door"
    },
    "haUrl": {
      "title": "Home Assistant URL",
      "required": true,
      "type": "string",
      "placeholder": "http://192.168.68.239:8123"
    },
    "haToken": {
      "title": "HA Long-Lived Token",
      "required": true,
      "type": "string",
      "format": "password"
    },
    "entityId": {
      "title": "HA Entity ID",
      "required": true,
      "type": "string",
      "default": "switch.Puerta1",
      "placeholder": "switch.Puerta1"
    }
  }
}

